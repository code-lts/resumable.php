{
    "name": "code-lts/resumable.php",
    "description": "PHP package for Resumable.js",
    "authors": [
        {
            "name": "xu ding",
            "email": "thedilab@gmail.com"
        },
        {
            "name": "William Desportes",
            "email": "williamdes@wdes.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/log": "^2 || ^3",
        "psr/http-message": "^1.0",
        "knplabs/gaufrette": "^0.11.1",
        "ondrej-vrto/php-filename-sanitize": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10 || ^11 || ^12",
        "wdes/coding-standard": "^3.0",
        "nyholm/psr7": "^1.4"
    },
    "scripts": {
        "phpunit": "@php phpunit",
        "phpcs": "@php phpcs",
        "phpcbf": "@php phpcbf"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ResumableJs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ResumableJs\\Test\\": "test/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
