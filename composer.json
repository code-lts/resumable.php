{
    "name": "code-lts/resumable.php",
    "description": "PHP package for Resumable.js",
    "authors": [
        {
            "name": "xu ding",
            "email": "thedilab@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "psr/log": "^1.1",
        "psr/http-message": "^1.0",
        "knplabs/gaufrette": "^0.11.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7 || ^8 || ^9",
        "wdes/coding-standard": "^3.0",
        "nyholm/psr7": "^1.4"
    },
    "scripts": {
        "phpunit": "@php phpunit",
        "phpcs": "@php phpcs",
        "phpcbf": "@php phpcbf"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ResumableJs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ResumableJs\\Test\\": "test/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
